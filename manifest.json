{
  "bundles": {
    "InterInvest\\ClientApiPer\\ClientApiPerBundle": ["all"]
  },
  "copy-from-recipe": {
    "config/": "%CONFIG_DIR%/"
  },
  "composer-scripts": {
    "cache:clear": "symfony-cmd"
  },
  "env": {
    "APP_ENV": "dev",
    "APP_SECRET": "%generate(secret)%"
  },
  "gitignore": [
    ".env",
    "/public/bundles/",
    "/var/",
    "/vendor/"
  ]
}
